{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "dotenv nodemon ./src/index.ts",
    "start": "node ./bin/www",
    "db:migration:revert": "per-env",
    "db:migration:revert:development": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "db:migration:revert:production": "node ./node_modules/typeorm/cli.js migration:revert",
    "db:migration:run": "per-env",
    "db:migration:run:development": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db:migration:run:production": "node ./node_modules/typeorm/cli.js migration:run",
    "db:schema:drop": "per-env",
    "db:schema:drop:development": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "db:schema:drop:production": "node ./node_modules/typeorm/cli.js schema:drop",
    "db:seed:run": "per-env",
    "db:seed:run:development": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "db:seed:run:production": "node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "per-env": "^1.0.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.45",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^4.1.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.10.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
